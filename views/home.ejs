<% include partials/header %>

<main class="main-content">
  <% for(var i = 0; i < articles.length; i++){ %>
    <section class="links-section <%= siteInfo(articles[i]._id).name.toLowerCase().replace(/ /g, "-") %>">
      <h2 class="links-header">
        <a href="<%= siteInfo(articles[i]._id).URL %>">
          <%= siteInfo(articles[i]._id).name %>
        </a>
      </h2>
      <% for(var j = 0; j < articles[i].data.length; j++){ %>
      <ul class="links-list">
        <li>
          <a href="<%= articles[i].data[j].link %>" class="headline" data-tippy-content="<%= articles[i].data[j].date %> <%= articles[i].data[j].summary %>">
            <%= articles[i].data[j].headline %>
          </a>
        </li>
      </ul>
      <% } %>
    </section>
  <% } %>
  </main>


<% include partials/footer %>